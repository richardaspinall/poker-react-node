{
  "handlerName": "PokerTablesJoin",
  "domainName": "poker-tables",
  "apiVerb": "join",
  "httpMethod": "post",
  "authRequired": true,
  "payload": {
    "properties": [
      {
        "name": "selectedSeatNumber",
        "type": "string",
        "required": true
      }
    ]
  },
  "output": {
    "properties": [
      {
        "name": "ok",
        "type": "boolean",
        "required": true
      }
    ]
  },
  "errors": [
    {
      "errorName": "PokerTableDoesNotExistError",
      "errorCode": "poker_table_does_not_exist",
      "message": "The specified poker table does not exist.",
      "classFile": "PokerTableDoesNotExistError.ts"
    }
  ]
}
