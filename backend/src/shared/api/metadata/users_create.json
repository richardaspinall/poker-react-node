{
    "handlerName": "UsersCreate",
    "domainName": "users",
    "apiVerb": "create",
    "httpMethod": "post",
    "authRequired": true,
    "payload": {
      "properties": [
        {
          "name": "username",
          "type": "string",
          "required": true
        },
        {
            "name": "password",
            "type": "string",
            "required": true
        }
      ]
    },
    "output": {
      "properties": [
        {
          "name": "ok",
          "type": "boolean",
          "required": true
        }
      ]
    },
    "errors": [
      {
        "errorName": "UsernameTakenError",
        "errorCode": "username_taken",
        "message": "Username already taken"
      },
      {
        "errorName": "UsersCreateError",
        "errorCode": "users_create_error",
        "message": "User not created"
      }
    ]
  }
  