import { ApiMethodShape } from './APIMethodShape';

export const ApiMethods: { [key: string]: ApiMethodShape } = {
  <% apiMethods.forEach(function(method) { %>
    <%= method.apiVerb %>: {
        httpMethod: '<%= method.httpMethod %>',
        path: '<%= method.domainName %>.<%= method.apiVerb %>',
        handler: '../../handlers/<%= method.domainName %>/<%= method.handlerName %>Handler.ts',
        handlerName: '<%= method.handlerName %>Handler',
    },
  <% }) %>
} as const;
